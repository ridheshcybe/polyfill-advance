{
  "name": "polyfill-advance",
  "version": "0.0.3",
  "description": "polyfill that add's beta functions that modern js users use",
  "main": "./dist/index.js",
  "bundle": "./dist/bundle.js",
  "files": [
    "app/",
    "dist/"
  ],
  "maintainers": [
    "ridhesh w",
    "bryce d",
    "daniel f",
    "regina f"
  ],
  "keywords": [
    "cybe",
    "ridhesh w"
  ],
  "repository": "https://github.com/cybemachine/polyfill-advance.git",
  "author": "cybe <https://www.github.com/cybemachine>",
  "license": "MIT",
  "private": false,
  "scripts": {
    "prepublish": "yarn run build && yarn run test",

    "dirbuild": "tsc --project ./config/dir.json",
    "amdbuild": "tsc --project ./config/amd.json",
    "systembuild": "tsc --project ./config/system.json",
    "build": "yarn run dirbuild && yarn run amdbuild && yarn run systembuild && node builder",

    "test": "node --experimental-vm-modules node_modules/jest/bin/jest.js"
  },
  "devDependencies": {
    "@types/jest": "^27.4.1",
    "@types/node": "^17.0.21",
    "jest": "^27.5.1"
